generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
  rooms    Room[]
}

model Room {
  id        Int    @id @default(autoincrement())
  name      String @unique
  size      Int
  note      String?
  user      User   @relation(fields: [user_id], references: [id])
  user_id   Int
}

model Reserve {
  id       Int    @id @default(autoincrement())
  name     String
  dateInit String
  dateEnd  String
}
